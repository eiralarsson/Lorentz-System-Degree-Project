    "function PointsSolutions(p,Δt,N,Solver,M,points)\n",
    "    solutions = []\n",
    "    for i = 1:M\n",
    "        x = points[:,i] + displace\n",
    "        X = SolutionLorenz(p, Δt, x, N, Solver)\n",
    "        push!(solutions, X)\n",
    "    end\n",
    "    return solutions\n",
    "end\n",
    "\n",
    "function PointsSolutionsJuliaSolver(p,Δt,N,Solver,M,points)\n",
    "    solutions = []\n",
    "    for i = 1:M\n",
    "        x = points[:,i]\n",
    "        prob = ODEProblem(LorentzSystem,x,(0,Δt*N),[θ,μ,β])\n",
    "        X = solve(prob, Solver())\n",
    "        push!(solutions, X)\n",
    "    end\n",
    "    return solutions\n",
    "end"
   ]
